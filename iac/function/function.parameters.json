{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "alwaysOn": {
      "value": true
    },
    "clientAffinityEnabled": {
      "value": false
    },
    "ftpsState": {
      "value": "FtpsOnly"
    },
    "netFrameworkVersion": {
      "value": "v6.0"
    },    
    "perSiteScaling": {
      "value": false
    },
    "publicNetworkAccess": {
      "value": "Enabled"
    },
    "serverOS": {
      "value": "Windows"
    },
    "skuCapacity": {
      "value": 1
    },
    "skuName": {
      "value": "B1"
    },
    "skuTier": {
      "value": "Basic"
    },
    "use32BitWorkerProcess": {
      "value": false
    },    
    "vnetRouteAllEnabled": {
      "value": true
    },
    "zoneRedundant": {
      "value": false
    }
  }
}